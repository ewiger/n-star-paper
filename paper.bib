@article{ForemanMagidorShelah1988,
 ISSN = {0003486X},
 URL = {http://www.jstor.org/stable/1971415},
 abstract = {The authors present a provably strongest form of Martin's axiom, called Martin's Maximum, and show its consistency. From it we derive the solutions to several classical problems in set theory, showing that 2ℵ 0 = ℵ2, the non-stationary ideal on ω1 is ℵ2-saturated, and several other results. We show as a consequence of our techniques that there can be no "nice" inner model of a supercompact cardinal. We generalize our results to cardinals above ω1 to show, for example, the consistency of the statement "The non-stationary ideal on every regular cardinal κ is precipitous."},
 author = {M. Foreman and M. Magidor and S. Shelah},
 journal = {Annals of Mathematics},
 number = {1},
 pages = {1--47},
 publisher = {Annals of Mathematics},
 title = {Martin's Maximum, Saturated Ideals, and Non-Regular Ultrafilters. Part I},
 urldate = {2024-05-16},
 volume = {127},
 year = {1988}
}


